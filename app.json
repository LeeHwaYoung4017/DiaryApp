{
  "expo": {
    "name": "DiaryApp",
    "slug": "diary-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "leehwayeong-diaryapp",
    "owner": "leehwayeong",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diaryapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.diaryapp"
    },
    "web": {},
    "plugins": [
      "expo-secure-store",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "64d9d81f-5482-425a-ba81-4cba130f0c9f"
      }
    }
  }
}
